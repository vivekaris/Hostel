<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.iotwebplanet.learn.hostel.Problem_actionActivity">

    <ImageView
        android:id="@+id/imageViewProblemView"
        android:padding="8sp"
        android:layout_width="@dimen/LayoutWidth368dp"
        android:layout_height="@dimen/LayoutHeightWardenMenu"
        app:srcCompat="@drawable/logo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:contentDescription="@string/Logo"
        app:layout_constraintBottom_toTopOf="@+id/reject"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:text="Name"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="162dp"
        android:id="@+id/textView10"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toTopOf="@+id/textView11" />

    <TextView
        android:id="@+id/nameTextViewPView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="name msg"
        android:textAlignment="textStart"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="259dp"
        tools:layout_editor_absoluteY="162dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/mobileTextViewPView" />

    <TextView

        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:gravity="start"
        android:text="Student ID"
        android:textSize="24sp"

        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="98dp"
        app:layout_constraintTop_toBottomOf="@+id/imageViewProblemView"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="8dp"
        android:id="@+id/textView9" />

    <TextView
        android:id="@+id/sIdTextViewPView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:text="Student Id msg"
        android:textSize="24sp"

        tools:layout_editor_absoluteX="214dp"
        tools:layout_editor_absoluteY="98dp"
        app:layout_constraintTop_toBottomOf="@+id/imageViewProblemView"
        app:layout_constraintBottom_toTopOf="@+id/comIdTextViewPView"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:text="Room Number"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="290dp"
        android:id="@+id/textView14"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toTopOf="@+id/textView15" />

    <TextView
        android:id="@+id/roomTextViewPView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:text="Room num msg"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="206dp"
        tools:layout_editor_absoluteY="290dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/statusTVPView" />

    <TextView

        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:gravity="start"
        android:text="Mobile Number"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="194dp"
        android:id="@+id/textView11"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toTopOf="@+id/textView12" />

    <TextView
        android:id="@+id/mobileTextViewPView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:text="Mobile Num msg"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="193dp"
        tools:layout_editor_absoluteY="194dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/hostelTextViewPView" />

    <TextView

        android:layout_width="99dp"
        android:layout_height="0dp"
        android:gravity="start"
        android:text="Hostel"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="226dp"
        android:id="@+id/textView12"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toTopOf="@+id/textView13" />

    <TextView
        android:id="@+id/hostelTextViewPView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:text="Hostel msg"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="253dp"
        tools:layout_editor_absoluteY="226dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/problemTypeTVPView" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:text="Problem Type"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="258dp"
        android:id="@+id/textView13"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toTopOf="@+id/textView14" />

    <TextView
        android:id="@+id/problemTypeTVPView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:text="Problem Type msg"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="175dp"
        tools:layout_editor_absoluteY="258dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/roomTextViewPView" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:text="Status"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="322dp"
        android:id="@+id/textView15"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toTopOf="@+id/textView16" />

    <TextView
        android:id="@+id/statusTVPView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:text="status Msg"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="256dp"
        tools:layout_editor_absoluteY="322dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/descTextViewPView" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:text="Description"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="354dp"
        android:id="@+id/textView16"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="69dp"
        app:layout_constraintBottom_toTopOf="@+id/resolve" />

    <TextView
        android:id="@+id/descTextViewPView"
        android:layout_width="178dp"
        android:layout_height="75dp"
        android:gravity="start"
        android:text="Description msg"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="198dp"
        tools:layout_editor_absoluteY="354dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginBottom="26dp"
        app:layout_constraintBottom_toTopOf="@+id/process" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:text="Com ID"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="130dp"
        android:id="@+id/textView2"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toTopOf="@+id/textView10" />

    <TextView
        android:id="@+id/comIdTextViewPView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:text="Com ID msg"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="244dp"
        tools:layout_editor_absoluteY="130dp"
        app:layout_constraintBottom_toTopOf="@+id/nameTextViewPView"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/resolve"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Resolve"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintEnd_toStartOf="@+id/reject"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp" />

    <Button
        android:id="@+id/process"
        android:layout_width="wrap_content"
        android:layout_height="49dp"
        android:text="Processing"

        app:layout_constraintStart_toEndOf="@+id/reject"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp" />

    <Button
        android:id="@+id/reject"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Reject"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/resolve"
        app:layout_constraintEnd_toStartOf="@+id/process"
        android:layout_marginBottom="8dp" />


</android.support.constraint.ConstraintLayout>