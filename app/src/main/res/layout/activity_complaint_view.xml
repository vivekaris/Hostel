<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.iotwebplanet.learn.hostel.Complaint_viewActivity">


    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Status"
        android:textSize="24sp"
        android:gravity="center"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="218dp"
        android:id="@+id/textView8"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintEnd_toStartOf="@+id/st"
        android:layout_marginRight="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="353dp" />

    <TextView
        android:id="@+id/st"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="status"
        android:gravity="left"
        android:textSize="24sp"
        tools:layout_editor_absoluteY="218dp"
        tools:layout_editor_absoluteX="258dp"
        android:layout_marginRight="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginLeft="16dp"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="353dp" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:gravity="center"
        android:text="Name"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="101dp"
        app:layout_constraintEnd_toStartOf="@+id/nameCv"
        android:layout_marginRight="16dp"
        android:id="@+id/textView19"
        android:layout_marginBottom="85dp"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="100dp" />

    <TextView
        android:id="@+id/nameCv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Name"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="72dp"
        tools:layout_editor_absoluteY="101dp"
        android:layout_marginRight="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginLeft="16dp"
        app:layout_constraintStart_toEndOf="@+id/textView19"
        android:layout_marginBottom="85dp"
        app:layout_constraintBottom_toTopOf="@+id/st"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="100dp" />
</android.support.constraint.ConstraintLayout>
